    "block_type": 2,
    "text": {
      "style": {
        "align": 1,
        "folded": false
      },
      "elements": [
        {
          "text_run": {
            "content": "问题：目前还是推荐的不够准。主要是套餐变换是很快并且比较多，市场部的规则变化也比较快，仅镇江常用的就有500+个在线套餐，这个问题不是单纯AI能解决的问题，套餐推荐的优化要跟市场部进来一起来做。",
            "text_element_style": {
              "bold": false,
              "italic": false,
              "strikethrough": false,
              "underline": false,
              "inline_code": false
            }
          }
        }
      ]
    }
  },
